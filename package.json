{
  "name": "@kuldeeps48/postgraphile-nest",
  "version": "1.1.2",
  "description": "Nestjs module for incorporating PostGraphile into your application",
  "author": "Alex Aldana",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kuldeeps48/postgraphile-nest.git"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc -p tsconfig.json",
    "precommit": "lint-staged",
    "test:ci": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "lint": "tslint -c tslint.json 'lib/**/*.ts'",
    "test": "jest --coverage",
    "prepare:publish": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next"
  },
  "dependencies": {
    "graphile-utils": "^4.10.0",
    "graphql-playground-middleware-express": "^1.7.22",
    "lodash": "^4.17.20",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@nestjs/common": "^8.4.4",
    "@nestjs/core": "^8.4.4",
    "@nestjs/platform-express": "^8.4.4",
    "@nestjs/testing": "^8.4.4",
    "@types/express": "^4.17.11",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.168",
    "@types/node": "^17.0.25",
    "@types/sinon": "^10.0.11",
    "@types/supertest": "^2.0.10",
    "coveralls": "^3.1.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.8",
    "nodemon": "^2.0.7",
    "postgraphile": "^4.10.0",
    "prettier": "^2.2.1",
    "sinon": "^13.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.4.4",
    "@nestjs/core": "^8.4.4",
    "postgraphile": "^4.12.9",
    "reflect-metadata": "^0.1.13"
  },
  "lint-staged": {
    "*.ts": []
  }
}
